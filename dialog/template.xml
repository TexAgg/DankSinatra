    <flow>
        <folder label="Main" id="Main">
			<output>
				<prompt selectionType="RANDOM">
					<item>
					Hello! I am Dank Sinatra. Ask me about pizza toppings.
					Or, type '\quit' to quit this conversation.
					</item>
				</prompt>
				<goto ref="getUserInput_2449614"></goto>
			</output>
			<getUserInput id="getUserInput_2449614">
				<search ref="folder_2449611"></search>
				<default>
					<output>
						<prompt selectionType="RANDOM">
							<item>
								I'm sorry, I did not understand your question.
								Please ask again.
							</item>
						</prompt>
					</output>
				</default>
			</getUserInput>
		</folder>
        <folder label="Library">
            <folder label="Live Content" id="folder_2449611">
				<input>
					<grammar>
						<item>What type of toppings do you have?</item>
						<item>$ What type of toppings do you have?</item>
						<item>$ list of toppings</item>
						<item>What * toppings * have</item>
						<item>$ list toppings * available</item>
						<item>$ what toppings do you have</item>
					</grammar>
				</input>
				<output>
					<prompt selectionType="RANDOM">
						<item>We have pizza</item>
					</prompt>
				</output>
			</folder>
            <folder label="Storage" id="Storage"></folder>
        </folder>
        <folder label="Concepts" id="Concepts"></folder>
    </flow>
